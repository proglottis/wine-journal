<% content_for :title do %>redwine.nothing.co.nz<% end %>

<% @ratings.each do |rating| %>
<div class="box"><%= rating.users.order('name').collect{|u| u.name}.join(' and ') %>  rated <%= link_to rating.wine.name, rating.wine %>, <%= time_ago_in_words rating.created_at %> ago.</div>
<% end %>

<%= will_paginate @ratings %>
